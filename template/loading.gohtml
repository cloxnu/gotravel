{{ define "loading" }}
    <div id="loading-wrapper">
        <span id="loading-shadow"></span>
        <span id="loading-shelter"></span>
        <div id="loading-div">
            <img src="{{ .Conf.BaseUrl }}{{ .Res.Home.dog }}" id="loading-img"/>
            <span id="loader">
                <span id="loader-inner"></span>
            </span>
        </div>
    </div>
{{ end }}
